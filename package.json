{
  "name": "depsman",
  "repository": "github:fabiospampinato/depsman",
  "description": "Extract and report metadata about dependencies of the current package.",
  "version": "1.1.0",
  "type": "module",
  "bin": "dist/bin.js",
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "tsex clean",
    "compile": "tsex compile",
    "compile:watch": "tsex compile --watch",
    "prepublishOnly": "npm run clean && npm run compile"
  },
  "keywords": [
    "metadata",
    "dependencies",
    "licenses",
    "reports"
  ],
  "dependencies": {
    "fetch-shim": "^1.1.0",
    "is": "npm:@fabiospampinato/is@^2.3.0",
    "sanitize-basename": "^2.0.0",
    "specialist": "^0.6.1",
    "string-escape-regex": "^1.0.0",
    "tiny-colors": "^2.0.1",
    "tiny-readdir": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "tiny-parse-argv": "^1.0.2",
    "tsex": "^1.1.3",
    "typescript": "^4.9.3"
  }
}
